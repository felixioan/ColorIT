<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ManageMembersViewController">
    <children>
      <HBox>
         <children>
              <Label prefHeight="26.0" prefWidth="68.0" text="Name:" />
              <TextField fx:id="name" prefHeight="26.0" prefWidth="546.0" />
         </children>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
              <Label prefHeight="26.0" prefWidth="52.0" text="Email:" />
            <TextField fx:id="email" prefHeight="26.0" prefWidth="529.0" />
         </children>
      </HBox>
        <HBox prefWidth="600.0">
            <children>
                <Button mnemonicParsing="false" onAction="#save" text="Save">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin></Button>
                <Button mnemonicParsing="false" onAction="#cancel" text="Cancel">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin></Button>
            </children>
         <padding>
            <Insets bottom="10.0" left="52.0" top="10.0" />
         </padding>
        </HBox>
        <Label fx:id="errorLabel" text="error label" textFill="RED" />
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>

</VBox>
